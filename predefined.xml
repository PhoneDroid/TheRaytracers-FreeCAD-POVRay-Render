<?xml version="1.0"?>
<Predefined>
    <Glass>
        <Predef material="M_Glass">Window glass</Predef>
        <Predef material="M_Glass2">Plexiglas</Predef>
        <Predef material="M_Glass3">Crystal glass</Predef>
        <Predef inc="glass.inc" material="M_Glass3" pigment="Col_Glass_Winebottle" interior="I_Glass_Dispersion1">Winebottle glass</Predef>
        <Predef inc="glass.inc" material="M_Glass" pigment="Col_Glass_Beerbottle">Beerbottle glass</Predef>
        <Predef inc="glass.inc" texture="T_Glass3" normal="bumps 1" interior="ior 1.5">Milky glass</Predef>
    </Glass>
    <Metal>
        <polished>
            <Predef inc="metals.inc" texture="Chrome_Metal">Chrome</Predef>
            <Predef inc="metals.inc" texture="Silver_Metal">Silver</Predef>
            <Predef inc="metals.inc" texture="Brass_Metal">Brass</Predef>
            <Predef inc="metals.inc" texture="Bronze_Metal">Bronze</Predef>
            <Predef inc="metals.inc" texture="Copper_Metal">Copper</Predef>
            <Predef inc="metals.inc" texture="Gold_Metal">Gold</Predef>
        </polished>
        <sandblast>
            <Predef inc="metals.inc" texture="T_Chrome_1A" finish="crand 0.5">Chrome</Predef>
            <Predef inc="metals.inc" texture="Bronze_Metal" finish="crand 0.5">Bronze</Predef>
        </sandblast>
        <brushed>
            <Predef inc="metals.inc" texture="Brushed_Aluminum">Aluminium</Predef>
        </brushed>
        <oxidized>
            <Predef inc="metals.inc" texture="Brass_Valley">old Brass</Predef>
            <Predef inc="metals.inc" texture="Spun_Brass">Spun Brass</Predef>
            <Predef inc="metals.inc" texture="Rusty_Iron">Rusty iron</Predef>
        </oxidized>
        <tryThis>
            <Predef material="" texture="T_Chrome_1A" pigment="color Red" normal="bumps 0.25" inc="metals.inc">My super metal</Predef>
        </tryThis>
    </Metal>
    <Stone>
        <Predef pigment="White_Marble">White Marble</Predef>
        <Predef pigment="color Black" finish="ambient 0.35&#10;brilliance 2&#10;diffuse 0.3&#10;metallic&#10;specular 0.80&#10;roughness 1/20&#10;reflection 0.1">Shungit</Predef>
    </Stone>
    <Wood>
        <Predef inc="woods.inc" texture="T_Wood7">Generic Wood</Predef>
        <Predef texture="EMBWood1">Walnut</Predef>
        <Predef texture="Cherry_Wood">Cherry</Predef>
        <Predef texture="Pine_Wood">Pine</Predef>
        <Predef texture="Rosewood">Rosewood</Predef>
        <Predef texture="DMFLightOak">Oak</Predef>
    </Wood>
    <Floor>
        <!-- We used the parquet texture for the floor from https://wiki.freecadweb.org/Tutorial_FreeCAD_POV_ray and adapted it for this specific syntax. Thanks to the creators of the tutorial. The tutorial is licensed under CC 3.0: https://creativecommons.org/licenses/by/3.0/ -->
        <Predef inc="woods.inc" pigment="wood color_map { M_Wood8A }turbulence 0.5 scale&lt;200, 20, 20>" finish="ambient 0.0 reflection 0.1 specular 0.2" normal="average normal_map {[1 gradient y 2 slope_map { [0&lt;0,1>][0.05 &lt;1,0>][0.95 &lt;1,0>][1 &lt;0,-1>] } scale 80][1 wood 0.5 slope_map { [0 &lt;0,0>][0.5 &lt;0.5,1>][1 &lt;1,0>] }turbulence 0.5 scale &lt;200, 20, 20>]}" comment="You have to put this texture on a big object to see how it really looks (e.g. 1mx1m surface)">Parquet</Predef>
    </Floor>
    <Water>
        <predef texture="Polished_Chrome" inc="textures.inc">Opaque, flat, reflecting water</predef>
        <predef texture="Polished_Chrome" normal="bumps 0.03 scale &lt;1,0.25,0.25>*1 turbulence 0.6" inc="textures.inc">Moderatly moved reflecting water</predef>
        <predef texture="Polished_Chrome" normal="crackle 0.15 scale 0.75 turbulence 0.3 translate &lt;-1,0,5>" inc="textures.inc">Moved, reflecting water</predef>
        <predef texture="Polished_Chrome" normal="bozo 1.75 scale &lt;2.0,1,0.3>*0.20 rotate &lt;0,10,0> turbulence 0.9 translate &lt;0,0,-2>" inc="textures.inc">By wind moved, reflecting water</predef>
    </Water>
    <Pattern>
        <Predef pigment="checker color White color Black">Checker</Predef>
        <Predef pigment="tiling 1 color_map{[0 White][0.95 White][0.98 Black][1 Black]} scale 2">Tiles</Predef>
        <Predef pigment="brick color White color rgb &lt;0.9, 0.1, 0.1&gt; mortar 0.1" normal="wrinkles 0.75 scale 0.01" finish="diffuse 0.9 phong 0.2">Red Bricks</Predef>
        <Predef pigment="gradient &lt;1,0,1>color_map{[ 0.0 color rgbf&lt;1,1,1,1> ][ 0.5 color rgbf&lt;1,1,1,1> ][ 0.5 color rgb&lt;1,0,0> ][ 1.0 color rgb&lt;1,0,0> ]}">Gradient Peel</Predef>
    </Pattern>
    <Media>
        <Predef media="emission color rgb&lt;1.0, 0.75, 0.1>*2.75 intervals 3 samples 32 method 3 aa_threshold 0.1 aa_level 5 density{ crackle  turbulence 0.3 color_map {[0 rgb 0][0.00 rgb 0][0.07 rgb 1][0.10 rgb 1][0.12 rgb 0][1.00 rgb 0]}}density{ spherical color_map {[0 rgb 0][0.7 rgb 1][1 rgb 1]}}" pigment="color rgbf&lt;1,1,1,1>" comment="You have to scale this texture to the size of the object. If you have an object with the dimensions 200mmx50mmx10mm, then you have to put this dimensions into the scale boxes above.">Fire</Predef>
        <Predef media="emission&#10;&#9;&#9;&#9;color rgb&lt;1.0, 0.25, 1,0>&#10;&#9;&#9;&#9;intervals 3&#10;&#9;&#9;&#9;samples 32&#10;&#9;&#9;&#9;method 3&#10;&#9;&#9;&#9;aa_threshold 0.1&#10;&#9;&#9;&#9;aa_level 5&#10;&#9;&#9;&#9;density {&#10;&#9;&#9;&#9;&#9;spherical&#10;&#9;&#9;&#9;&#9;color_map {&#10;&#9;&#9;&#9;&#9;&#9;[0 rgb 0]&#10;&#9;&#9;&#9;&#9;&#9;[0.7 rgb 0.4]&#10;&#9;&#9;&#9;&#9;&#9;[1 rgb 0.5]&#10;&#9;&#9;&#9;&#9;}&#10;&#9;&#9;&#9;}&#10;&#9;&#9;" pigment="color rgbf&lt;1,1,1,1>">Plasma</Predef>
    </Media>
</Predefined>

